# Model configurations
MULTISTEP_MODEL_PROVIDER=openai
MULTISTEP_MODEL_NAME=gpt-4

TOOL_CALLING_MODEL_PROVIDER=anthropic
TOOL_CALLING_MODEL_NAME=claude-3-opus-latest

CODE_MODEL_PROVIDER=gemini
CODE_MODEL_NAME=gemini-pro

# LLM API Keys (Required for specific models)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Tool API Keys (Required for specific prebuilt tools)
WEATHERAPI_API_KEY=your_weatherapi_com_key_here
IPGEOLOCATION_API_KEY=your_ipgeolocation_io_key_here
EXCHANGERATE_API_KEY=your_exchangerate_api_key_here
API_NINJA_API_KEY=your_api_ninja_key_here # Note: Free tier may have limitations (e.g., no city lookup for weather/timezone)
JINA_API_KEY=your_jina_reader_api_key_here # Used for read_webpage tool
BRIGHTDATA_PROXY_URL=http://user:pass@proxy.example.com:port # Used for search_google tool 