{
    "wstl": {
        "title": "Settings objects",
        "actions": [
            {
                "name": "heaserver-settings-settings-object-get-properties",
                "description": "View and edit this settings object's properties",
                "type": "unsafe",
                "target": "item cj-template",
                "action": "update",
                "prompt": "Properties",
                "inputs": [
                    {
                        "name": "id",
                        "prompt": "Id",
                        "readOnly": true
                    },
                    {
                        "name": "display_name",
                        "prompt": "Name",
                        "required": true
                    },
                    {
                        "name": "description",
                        "prompt": "Description",
                        "type": "textarea"
                    },
                    {
                        "name": "owner",
                        "prompt": "Owner",
                        "required": true,
                        "type": "select",
                        "hea": {
                            "optionsFromUrl": {
                                "path": "people/",
                                "value": "id",
                                "text": "display_name"
                            }
                        }
                    },
                    {
                        "name": "created",
                        "prompt": "Created",
                        "readOnly": true,
                        "hea": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "modified",
                        "prompt": "Modified",
                        "readOnly": true,
                        "hea": {
                            "type": "datetime"
                        }
                    },
                    {
                        "name": "source",
                        "prompt": "Source"
                    },
                    {
                        "hea": {
                            "section": "user_shares",
                            "sectionPrompt": "User permissions",
                            "optionsFromUrl": {
                                "path": "people/",
                                "value": "id",
                                "text": "display_name"
                            }
                        },
                        "name": "user",
                        "prompt": "User",
                        "required": true,
                        "type": "select"
                    },
                    {
                        "hea": {
                            "section": "user_shares",
                            "cardinality": "multiple"
                        },
                        "name": "permissions",
                        "prompt": "Permissions",
                        "type": "select",
                        "suggest": [
                            {"value":  "COOWNER", "text":  "Co-owner"},
                            {"value":  "DELETER", "text":  "Deleter"},
                            {"value":  "EDITOR", "text":  "Editor"},
                            {"value":  "SHARER", "text":  "Sharer"},
                            {"value":  "VIEWER", "text":  "Viewer"}
                        ]
                    },
                    {
                        "name": "type",
                        "prompt": "Type",
                        "required": true,
                        "readOnly": true,
                        "value": "heaobject.root.ShareImpl",
                        "hea": {
                            "display": false,
                            "section": "user_shares"
                        }
                    },
                    {
                        "hea": {
                            "section": "group_shares",
                            "sectionPrompt": "Group permissions",
                            "optionsFromUrl": {
                                "path": "groups/",
                                "value": "id",
                                "text": "display_name"
                            }
                        },
                        "name": "group",
                        "prompt": "Group",
                        "required": true,
                        "type": "select"
                    },
                    {
                        "hea": {
                            "section": "group_shares",
                            "cardinality": "multiple"
                        },
                        "name": "permissions",
                        "prompt": "Permissions",
                        "type": "select",
                        "suggest": [
                            {"value":  "COOWNER", "text":  "Co-owner"},
                            {"value":  "DELETER", "text":  "Deleter"},
                            {"value":  "EDITOR", "text":  "Editor"},
                            {"value":  "SHARER", "text":  "Sharer"},
                            {"value":  "VIEWER", "text":  "Viewer"}
                        ]
                    },
                    {
                        "name": "type",
                        "prompt": "Type",
                        "required": true,
                        "readOnly": true,
                        "value": "heaobject.root.ShareImpl",
                        "hea": {
                            "display": false,
                            "section": "group_shares"
                        }
                    }
                ]
            },
            {
                "name": "heaserver-settings-settings-object-get-open-choices",
                "description": "Open this settings object",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Open"
            },
            {
                "name": "heaserver-settings-settings-object-get-self",
                "description": "View this settings object",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "View"
            },
            {
                "name": "heaserver-settings-settings-object-get-actual",
                "description": "Get the actual object",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Get actual"
            },
            {
                "name": "heaserver-settings-settings-object-open",
                "description": "Open",
                "type": "safe",
                "action": "read",
                "target": "item read cj",
                "prompt": "Open"
            }
        ]
    }
}
