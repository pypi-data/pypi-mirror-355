{
  "name": "jupyter_pieces",
  "version": "2.0.0",
  "description": "Never forget again with Pieces: AI-powered workstream context awareness and material management",
  "keywords": [
    "pieces",
    "pieces-for-developers",
    "pieces-extension",
    "code",
    "snippet",
    "snippets",
    "materials",
    "code-snippet",
    "code-snippets",
    "code-snippet-manager",
    "code-snippet-management",
    "code-snippet-management-tool",
    "code-snippet-management-tool-for-developers"
  ],
  "homepage": "https://old.docs.pieces.app/extensions-plugins/jupyterlab",
  "bugs": {
    "url": "https://github.com/pieces-app/support/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Pieces for Developers",
    "email": "support@pieces.app"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/pieces-app/jupyter_pieces.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:css && jlpm build:labextension:dev",
    "build:css": "npx tailwindcss -i ./style/extension.css -o ./style/index.css",
    "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "build:r": "jlpm build:lib && jlpm build:css && jlpm build:labextension:dev && pip install -ve . --user",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf jupyter_pieces/labextension jupyter_pieces/_version.py",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "jlpm build",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "watch": "run-p watch:src watch:css watch:labextension",
    "watch:src": "tsc -w --sourceMap",
    "watch:css": "npx tailwindcss -i ./style/extension.css -o ./style/index.css --watch",
    "watch:labextension": "jupyter labextension watch .",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "version": "node version-bump.mjs",
    "staging-release": "standard-version --prerelease staging",
    "patch-release": "standard-version --release-as patch",
    "minor-release": "standard-version --release-as minor",
    "major-release": "standard-version --release-as major"
  },
  "dependencies": {
    "@jupyter-widgets/jupyterlab-manager": "^5.0.7",
    "@jupyterlab/application": "^4.0.1",
    "@jupyterlab/apputils": "^4.1.2",
    "@jupyterlab/cell-toolbar": "^4.0.2",
    "@jupyterlab/cells": "^4.0.2",
    "@jupyterlab/codeeditor": "^4.0.2",
    "@jupyterlab/docregistry": "^4.0.2",
    "@jupyterlab/fileeditor": "^4.0.2",
    "@jupyterlab/markdownviewer": "^4.0.2",
    "@jupyterlab/notebook": "^4.0.2",
    "@jupyterlab/rendermime": "^4.0.2",
    "@jupyterlab/rendermime-interfaces": "^3.8.2",
    "@jupyterlab/settingregistry": "^4.0.0",
    "@jupyterlab/statedb": "^4.0.3",
    "@jupyterlab/terminal": "^4.0.9",
    "@jupyterlab/ui-components": "^4.0.1",
    "@lumino/widgets": "^2.1.1",
    "@pieces.app/pieces-os-client": "^4.1.0",
    "@sentry/browser": "^7.56.0",
    "@types/marked": "^5.0.0",
    "crypto-js": "^4.2.0",
    "js-search": "^2.0.1",
    "lunr": "^2.3.9",
    "marked": "^5.1.0",
    "md-block": "^0.0.1",
    "npm-run-all": "^4.1.5",
    "p-retry": "^5.1.2",
    "prismjs": "^1.29.0",
    "run-p": "^0.0.0",
    "sanitize-html": "^2.13.0",
    "semver": ">=7.5.2",
    "uuid": "^9.0.0",
    "word-wrap": ">=1.2.4"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "@types/codemirror": "^5.60.8",
    "@types/crypto-js": "^4.1.1",
    "@types/js-search": "^1.4.0",
    "@types/json-schema": "^7.0.11",
    "@types/lunr": "^2.3.6",
    "@types/node": "^20.2.5",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.26",
    "@types/sanitize-html": "^2.9.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "pinst": ">=2",
    "postcss": "^8.4.27",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "source-map-loader": "^1.0.2",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.5.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "~5.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "yjs": "^13.5.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyter_pieces/labextension",
    "schemaDir": "schema",
    "_build": {
      "load": "static/remoteEntry.3fac9ba48f9b22d23c96.js",
      "extension": "./extension",
      "style": "./style"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.d.ts",
    "src/PiecesSDK"
  ],
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "args": "none"
        }
      ],
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-namespace": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "prefer-arrow-callback": "error"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-standard",
      "stylelint-prettier/recommended"
    ],
    "rules": {
      "property-no-vendor-prefix": null,
      "selector-no-vendor-prefix": null,
      "value-no-vendor-prefix": null
    }
  },
  "lint-staged": {
    "*.{js,ts,css,md}": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "standard-version": {
    "t": ""
  }
}
